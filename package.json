{
  "name": "sticker-dl",
  "version": "1.0.1",
  "author": "Livingston <livingston@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 14"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "bin": {
    "sticker-dl": "./bin/run"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sticker-dl",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && pkg-ok",
    "postpublish": "pinst --enable",
    "prepack": "yarn compile && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint . --ext .ts --config .eslintrc.js",
    "compile": "rm -rf lib && tsc -b "
  },
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.1",
    "cheerio": "1.0.0-rc.5",
    "colorette": "1.2.1",
    "fs-extra": "9.1.0",
    "listr": "0.14.3",
    "node-fetch": "2.6.1",
    "please-upgrade-node": "3.2.0",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@oclif/dev-cli": "1.26.0",
    "@oclif/test": "1.2.8",
    "@types/cheerio": "0.22.23",
    "@types/fs-extra": "9.0.6",
    "@types/listr": "0.14.2",
    "@types/node": "14.14.22",
    "@types/node-fetch": "2.5.8",
    "commitizen": "4.2.3",
    "commitlint": "11.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.19.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.2.0",
    "husky": "5.0.8",
    "pinst": "2.1.4",
    "pkg-ok": "2.3.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "resolutions": {
    "eslint-plugin-unicorn": "^6.0.1|| 27.0.0"
  }
}
